                  <h3>Cinema</h3>
                  <button class="btn btn-info">12:30</button>
                  <a class="btn btn-info" href="#">12:30</a>
                  <button class="btn btn-info">14:00</button>
                </div>
              </div>
              
            </div>
            <% }) %>
